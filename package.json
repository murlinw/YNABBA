{
  "name": "ynabba",
  "description": "Building a Betta YNAB with MySql, Express, AngularJS, and Node.js.",
  "version": "1.0.0",
  "private": false,
  "author": "https://github.com/dLopreiato/YNABBA/graphs/contributors",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server --c bs-config.json\" ",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint src/app/**/*.ts src/app/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "win_lint_modules": "for /r \\modules %f in (*.ts) do tslint %f -t verbose"
  },
  "dependencies": {
    "angular2-in-memory-web-api": "0.0.17",
    
    "core-js": "^2.4.0",
    "express": "^4.10.6",
    "mysql": "^2.5.4",
    "passport": "^0.3.2",

    "compression": "1.6.2"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "bootstrap": "^3.3.6",

    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12",
    "reflect-metadata": "^0.1.3",

    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "tslint": "^3.7.4",
    "gulp" : "3.9.1",
    "gulp-sourcemaps": "2.0.0-alpha",
    "del" : "2.2.0",
    "gulp-print" : "2.0.1",
    "gulp-typescript" : "2.13.6",
    "gulp-jspm-build" : "0.0.15",
    "gulp-concat": "2.6.0",
    "gulp-uglify": "2.0.0",
    "gulp-minify": "0.0.14",
    "gulp-html-replace": "1.6.1",
    "pump": "1.0.1"
  }
}
