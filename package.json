{
  "name": "ynabba",
  "description": "Building a Betta YNAB with MySql, Express, AngularJS, and Node.js.",
  "version": "1.0.0",
  "private": false,
  "author": "https://github.com/dLopreiato/YNABBA/graphs/contributors",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint modules/*/client/**/*.ts modules/*/client/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.17",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "express": "^4.10.6",
    "mysql": "^2.5.4",
    "passport": "^0.3.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12",
    "reflect-metadata": "^0.1.3"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "tslint": "^3.7.4"
  }
}
